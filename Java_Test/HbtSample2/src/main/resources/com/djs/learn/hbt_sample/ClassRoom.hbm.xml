<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.djs.learn.hbt_sample.ClassRoom" table="classroom"
		catalog="hibernate">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="roomNumber" type="string" column="RoomNumber"
			length="45" not-null="true" />
		<set name="courses" cascade="all-delete-orphan" inverse="true">
			<key column="ClassRoomId" not-null="true" />
			<one-to-many class="com.djs.learn.hbt_sample.Course" />
		</set>
	</class>
</hibernate-mapping>
